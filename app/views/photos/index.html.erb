<h2>A Tumblr of Photos</h2>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Image</th>
  </tr>

<% @photos.each do |f| %>
  <tr>
    <td><%= f.name %></td>
    <td><%= f.description %></td>
    <td><%= image_tag f.image_url(:thumb).to_s %></td>
  </tr>
<% end %>
</table>

<%= link_to "New Photo", new_photo_path %>
